{
    "reporter": "datadog",
    "delay": 1000,
    "delayAfter": {
        "uptime": 60000,
        "certificate": 28800000
    },
    "checks": [
        {
            "type": ["uptime"],
            "url": "https://example.org"
        },
        {
            "type": ["certificate", "uptime"],
            "url": "https://example.com",
            "minDaysRemaining": 30
        }
    ]
}